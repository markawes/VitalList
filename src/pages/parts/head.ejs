<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<link rel="shortcut icon" href="/img/icon.png" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.4.2/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
<script async src="https://arc.io/widget.min.js#T7ANmTma"></script>
<link rel="stylesheet" id="theme" href="/assets/ocean.css">
<script src="https://kit.fontawesome.com/29e7757043.js" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/typeit@7.0.4/dist/typeit.min.js"></script>

<script>
$(document).ready(function() {
    const themeFromLocal = localStorage.getItem("theme");
    const themes = ["white", "dark", "ocean"];
    const currentTheme = themes.includes(themeFromLocal) ? themeFromLocal : "ocean";
    $("#theme").attr("href", `/assets/${currentTheme}.css`);
});
</script>