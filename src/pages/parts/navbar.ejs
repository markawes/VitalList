<head>
  <link rel="stylesheet" type="text/css" href="/assets/navbar.css" />
  <link rel="stylesheet" type="text/css" href="/assets/main.css" />
</head>
<nav class="navbar">
  <div class="logo"><a href="/"><img style="margin-left: -95;" src="/img/icon.png" alt="VitalList Logo" width="50" height="50"><p class="title" style="margin-top: -40px;margin-left: 75px;">VitalList </p></a></div>
  <ul class="nav-links">
      <input type="checkbox" id="checkbox_toggle" />
      <label for="checkbox_toggle" style="cursor: pointer;" class="hamburger">&#9776;</label>
    <div class="menu">
      <li style="margin-top: 4px"><a href="/servers"><i class="fa-solid fa-server"></i> Servers</a></li>
      <li style="margin-top: 4px"><a href="/partners"><i class="fa-solid fa-handshake"></i> Partners</a></li>
      <% if(user) { %>
        
          <% if(config.owners.includes(user.id)) { %>
          <li style="margin-top: 4px"><a href="/admin"><i class="fa fa-magic mr-1"></i> Admin</a></li>
          <% } %>
        <% } %>
      <% if (user) { %>
        <div style="margin: 10; font-size: 17px;"><i class="fa-solid fa-id-card"></i> <%= user.username %>#<%= user.discriminator %></div>
      <% } else { %>
        <li style="margin-top: 4px"><a href="/auth/login"><i class="fa-solid fa-sign-in-alt"></i> Login via Discord</a></li>
      <% } %>
      
    </div>
  </ul>
</nav>